    # Show key help
    bindsym $logo+Shift+Slash exec /home/amy/github/bin/keyhelp

    # Show key help
    bindsym $logo+Help exec /home/amy/github/bin/keyhelp

    # Lock screen
    bindsym Pause exec swaylock

    # Reload window manager
    bindsym Ctrl+$logo+r reload

    # Reload window manager
    bindsym Ctrl+$logo+Return reload

    # Exit window manager
    bindsym Ctrl+$logo+q exec swaynag -t warning -m 'Do you really want to exit sway?' -b 'Yes' 'swaymsg exit'

    # Show launch menu
    bindsym $logo+Return exec $menu

    # Launch terminal
    bindsym $logo+Shift+Return exec $term

    # Kill focused window
    bindsym $logo+Delete kill

    # Move focus left
    bindsym $logo+Left focus left

    # Move focus right
    bindsym $logo+Right focus right

    # Move focus up
    bindsym $logo+Up focus up

    # Move focus down
    bindsym $logo+Down focus down

    # Move window left
    bindsym $logo+Shift+Left move left

    # Move window right
    bindsym $logo+Shift+Right move right

    # Move window up
    bindsym $logo+Shift+Up move up

    # Move window down
    bindsym $logo+Shift+Down move down

    # Go to workspace 1
    bindsym $logo+f1 workspace 'WS_1'

    # Go to workspace 2
    bindsym $logo+f2 workspace 'WS_2'

    # Go to workspace 3
    bindsym $logo+f3 workspace 'WS_3'

    # Go to workspace 4
    bindsym $logo+f4 workspace 'WS_4'

    # Go to workspace 5
    bindsym $logo+f5 workspace 'WS_5'

    # Go to workspace 6
    bindsym $logo+f6 workspace 'WS_6'

    # Go to workspace 7
    bindsym $logo+f7 workspace 'WS_7'

    # Go to workspace 8
    bindsym $logo+f8 workspace 'WS_8'

    # Go to workspace 9
    bindsym $logo+f9 workspace 'WS_9'

    # Go to workspace 10
    bindsym $logo+f10 workspace 'WS_10'

    # Go to workspace 11
    bindsym $logo+f11 workspace 'WS_11'

    # Go to workspace 12
    bindsym $logo+f12 workspace 'WS_12'

    # Move window to workspace 1
    bindsym $logo+Shift+f1 move container to workspace 'WS_1'

    # Move window to workspace 2
    bindsym $logo+Shift+f2 move container to workspace 'WS_2'

    # Move window to workspace 3
    bindsym $logo+Shift+f3 move container to workspace 'WS_3'

    # Move window to workspace 4
    bindsym $logo+Shift+f4 move container to workspace 'WS_4'

    # Move window to workspace 5
    bindsym $logo+Shift+f5 move container to workspace 'WS_5'

    # Move window to workspace 6
    bindsym $logo+Shift+f6 move container to workspace 'WS_6'

    # Move window to workspace 7
    bindsym $logo+Shift+f7 move container to workspace 'WS_7'

    # Move window to workspace 8
    bindsym $logo+Shift+f8 move container to workspace 'WS_8'

    # Move window to workspace 9
    bindsym $logo+Shift+f9 move container to workspace 'WS_9'

    # Move window to workspace 10
    bindsym $logo+Shift+f10 move container to workspace 'WS_10'

    # Move window to workspace 11
    bindsym $logo+Shift+f11 move container to workspace 'WS_11'

    # Move window to workspace 12
    bindsym $logo+Shift+f12 move container to workspace 'WS_12'

    # Toggle fullscreen for currently focused window
    bindsym Ctrl+$logo+Shift+f fullscreen toggle

    # Move currently focused window to scratchpad
    bindsym $logo+Shift+minus move scratchpad

    # Show/hide next scratchpad window
    bindsym $logo+minus scratchpad show

    # Make window narrower
    bindsym Ctrl+$logo+Left resize shrink width 50px

    # Make window wider
    bindsym Ctrl+$logo+Right resize grow width 50px

    # Make window taller
    bindsym Ctrl+$logo+Up resize grow height 50px

    # Make window shorter
    bindsym Ctrl+$logo+Down resize shrink height 50px

